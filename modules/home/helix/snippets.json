{
  "Svelte 5 Template": {
    "prefix": "s5-template",
    "body": ["<script>", "  $1", "</script>", "", "$0"]
  },
  "Svelte 5 Template (TS)": {
    "prefix": "s5-template-ts",
    "body": ["<script lang=\"ts\">", "  $1", "</script>", "", "$0"]
  },
  "Svelte 5 Props": {
    "prefix": "s5-props",
    "body": ["const { $0 } = $props();"]
  },
  "Svelte 5 Props (TS)": {
    "prefix": "s5-props-ts",
    "body": ["const { $0 }: { $1 } = $props();"]
  },
  "Svelte 5 If": {
    "prefix": "s5-if",
    "body": ["{#if $1}", "\t$0", "{/if}"]
  },
  "Svelte 5 If Else": {
    "prefix": "s5-if-else",
    "body": ["{#if $1}", "\t$0", "{:else}", "$2", "{/if}"]
  }
}
